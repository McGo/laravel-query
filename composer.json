{
  "name": "mcgo/laravel-request-query-filter",
  "description": "Add eloquent query filters based on request query parameters",
  "authors": [
    {
      "name": "Mirko Haaser",
      "email": "laravel-request-query-filter@mcgo.dev"
    }
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0"
  },
  "require": {
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "McGo\\RequestQueryFilter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "McGo\\RequestQueryFilter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "McGo\\RequestQueryFilter\\RequestQueryFiltererviceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
